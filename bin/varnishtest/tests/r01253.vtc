varnishtest "restarting a pass in vcl_fetch should not panic"

server s1 {
	rxreq
	txresp -status 304 -body "1234"
	accept
	rxreq
	txresp -status 200 -body "56"
} -start

varnish v1 -vcl+backend {
	sub vcl_recv {
		return (pass);
	}
	sub vcl_fetch {
		if (beresp.status != 200) {
			return (restart);
		}
	}
} -start

client c1 {
	txreq 
	rxresp
	expect resp.bodylen == 2
} -run
